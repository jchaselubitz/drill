{
	"name": "my-project",
	"version": "0.0.1",
	"private": true,
	"type": "module",
	"scripts": {
		"dev": "vite dev  --host 0.0.0.0",
		"start": "vite start  --host http://192.168.0.103:3000",
		"stop": "supabase stop",
		"new": "rm -rf node_modules && yarn cache clean && yarn install",
		"fresh": "supabase db reset",
		"functions-run": "supabase functions serve --env-file ./supabase/.env.local --no-verify-jwt",
		"functions-deploy": "supabase functions deploy --project-ref fororflyhxeircqexslh",
		"functions-set-secret": "supabase secrets set --env-file ./supabase/.env.prod --project-ref fororflyhxeircqexslh",
		"pull-supa": "supabase db pull",
		"push-supa": "supabase db push",
		"create-migration": "supabase db diff --use-migra -f local_schema",
		"gen-local": "supabase gen types typescript --local",
		"build": "vite build",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"fix": "yarn run fix-js-lint && yarn run fix-style-lint",
		"format": "prettier --plugin-search-dir . --write .",
		"ngrok": "ngrok http --domain=romantic-pig-living.ngrok-free.app 3000",
		"build-staging": ". ./staging.env && houdini generate && vite build",
		"build-production": ". ./prod.env && houdini generate && vite build",
		"old-up": "docker-compose up -d",
		"old-fresh": "yarn install && docker-compose down -v --remove-orphans && docker-compose up --build -d && sleep 20 && yarn schema && yarn generate",
		"old-new": "yarn install && docker-compose down --remove-orphans && docker-compose up --build -d && sleep 20 && yarn schema && yarn generate",
		"old-stop": "docker-compose stop"
	},
	"dependencies": {
		"@fortawesome/fontawesome-svg-core": "^6.5.1",
		"@fortawesome/free-brands-svg-icons": "^6.5.1",
		"@fortawesome/free-regular-svg-icons": "^6.5.1",
		"@fortawesome/free-solid-svg-icons": "^6.5.1",
		"@supabase/auth-helpers-sveltekit": "^0.13.0",
		"@supabase/auth-ui-shared": "^0.1.8",
		"@supabase/auth-ui-svelte": "^0.2.8",
		"@supabase/supabase-js": "^2.43.5",
		"@sveltejs/vite-plugin-svelte": "^3.1.1",
		"anki-apkg-export": "^4.0.3",
		"audiobuffer-to-wav": "^1.0.0",
		"axios": "^1.7.2",
		"classnames": "^2.5.1",
		"fast-xml-parser": "^4.3.4",
		"openai": "^4.24.7",
		"path": "^0.12.7",
		"postcss-load-config": "^4.0.1",
		"svelte-awesome": "^3.3.1",
		"svelte-markdown": "^0.4.1"
	},
	"devDependencies": {
		"@sveltejs/adapter-netlify": "^4.2.1",
		"@sveltejs/adapter-node": "^2.0.2",
		"@sveltejs/kit": "^2.0.5",
		"@tailwindcss/forms": "^0.5.6",
		"@tailwindcss/typography": "^0.5.10",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"autoprefixer": "^10.4.16",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-svelte": "^2.30.0",
		"postcss": "^8.4.30",
		"prettier": "^2.8.0",
		"prettier-plugin-svelte": "^2.10.1",
		"supabase": "^1.176.10",
		"svelte": "^4.2.18",
		"svelte-check": "^3.6.2",
		"svelte-preprocess": "^6.0.1",
		"tailwindcss": "^3.3.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.0.10"
	}
}
