{
	"extends": "./.svelte-kit/tsconfig.json",
	"compilerOptions": {
					"allowJs": true,
					"checkJs": true,
					"esModuleInterop": true,
					"forceConsistentCasingInFileNames": true,
					"resolveJsonModule": true,
					"skipLibCheck": true,
					"sourceMap": true,
					"strict": true,
					"rootDirs": [
									".",
									"./.svelte-kit/types",
									"./$houdini/types"
					],
					"baseUrl": ".",
					"paths": {
									"$houdini/*": ["./houdini/*"],
									"$lib/*": ["./src/lib/*"],
									"$lib": ["./src/lib"],
									"src/*": ["./src/*"],
					}
	}
}
